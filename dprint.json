{
  // https://dprint.dev/config/#extending-a-different-configuration-file
  "extends": "https://raw.githubusercontent.com/BesLogic/shared-configs/main/dprint.json",
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "**/coverage", // created on install by nx
    "**/build",
    "**/cache",
    "**/dist",
    "**/tmp",
    "**/obj",
    // Don't touch submodules
    "TexConvert/",
    "PitfallARCTool/"
  ],
  "newLineKind": "lf"
}
